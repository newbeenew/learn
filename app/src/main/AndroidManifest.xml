<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="d.ql.account">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".account_book_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddCurrentActivity"
            android:parentActivityName=".account_book_main"></activity>
        <activity
            android:name=".accountDetailActivity"
            android:label="@string/title_activity_account_detail"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
